{
    "entryPoints": [
        "src/**/*.ts",
        "src/**/*.json"
    ],
    "name": "Kerby64",
    "readme": "README.md",
    "exclude": [
        "node_modules/**",
        "dist/**",
        "build/**",
        "coverage/**",
        "test/**",
        "tests/**",
        "*.spec.ts",
        "*.test.ts"
    ],
    "alwaysCreateEntryPointModule": false,
    "blockTags": [
        "@defaultValue",
        "@deprecated",
        "@example",
        "@param",
        "@privateRemarks",
        "@remarks",
        "@returns",
        "@see",
        "@throws",
        "@extends",
        "@implements",
        "@license",
        "@return",
        "@satisfies",
        "@type",
        "@todo",
        "@method"
    ],
    "customFooterHtmlDisableWrapper": true,
    "customFooterHtml": "<span style='display:flex;justify-content:flex-end;border-right-width:100vw'>Socrimoft (2025)</span>"
}